{
    "name": "madewithlove/notion-php-sdk",
    "description": "An unofficial PHP SDK for Notion",
    "license": "MIT",
    "require": {
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^7.0",
        "league/commonmark": "^0.19.2",
        "league/uri": "^6.0",
        "ramsey/uuid": "^4.0",
        "symfony/cache": "^5.0"
    },
    "require-dev": {
        "madewithlove/php-cs-fixer-config": "^1.10",
        "phpunit/phpunit": "^8.4",
        "roave/security-advisories": "dev-master",
        "symfony/dotenv": "^5.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Notion\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notion\\": "tests"
        }
    },
    "scripts": {
        "lint": "prettier --write '{src,examples,tests}/**/*' --print-width 120 --single-quote && php-cs-fixer fix -v",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
